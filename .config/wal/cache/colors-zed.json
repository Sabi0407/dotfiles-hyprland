{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#190a1990",
                "editor.background": "#190a1990",
                "editor.foreground": "#c5c1c5",
                "text": "#c5c1c5",
                "text.muted": "#c5c1c570",
                "text.ignored": "#c5c1c540",
                "text.placeholder": "#c5c1c550",
                "ignored": "#c5c1c530",
                "element.hover": "#c5c1c530",
                "ghost_element.hover": "#B04E7430",
                "ghost_element.selected": "#B04E7430",
                "ghost_element.active": "#B04E7460",
                "border": "#190a19",
                "editor.highlighted_line.background": "#B04E7410",
                "editor.active_line.background": "#B04E7410",
                "panel.background": "#190a1990",
                "title_bar.background": "#190a1990",
                "title_bar.inactive_background": "#190a1990",
                "status_bar.background": "#190a1990",
                "drop_target.background": "#190a1990",
                "elevated_surface.background": "#190a19",
                "toolbar.background": "#190a1990",
                "tab_bar.background": "#190a1990",
                "tab.inactive_background": "#190a1990",
                "tab.active_background": "#B04E7430",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#190a19",
                "scrollbar.thumb.background": "#B04E74",
                "editor.gutter.background": "#190a1990",
                "terminal.background": "#190a1910",
                "terminal.foreground": "#c5c1c5",
                "terminal.dim_foreground": "#c5c1c5",
                "terminal.bright_foreground": "#c5c1c5",
                "terminal.ansi.black": "#190a19",
                "terminal.ansi.red": "#B04E74",
                "terminal.ansi.green": "#DB576E",
                "terminal.ansi.yellow": "#3C6D8A",
                "terminal.ansi.blue": "#6E4787",
                "terminal.ansi.magenta": "#614883",
                "terminal.ansi.cyan": "#955089",
                "terminal.ansi.white": "#c5c1c5",
                "terminal.ansi.bright_black": "#6e596e",
                "terminal.ansi.bright_red": "#B04E74",
                "terminal.ansi.bright_green": "#DB576E",
                "terminal.ansi.bright_yellow": "#3C6D8A",
                "terminal.ansi.bright_blue": "#6E4787",
                "terminal.ansi.bright_magenta": "#614883",
                "terminal.ansi.bright_cyan": "#955089",
                "terminal.ansi.bright_white": "#c5c1c5",
                "modified": "#B04E74",
                "syntax": {
                    "attribute": {
                        "color": "#B04E74"
                    },
                    "boolean": {
                      "color": "#DB576E"
                    },
                    "comment": {
                      "color": "#6e596e"
                    },
                    "comment.doc": {
                      "color": "#6e596e"
                    },
                    "constant": {
                      "color": "#DB576E"
                    },
                    "function": {
                      "color": "#955089"
                    },
                    "keyword": {
                      "color": "#6E4787"
                    },
                    "number": {
                      "color": "#614883"
                    },
                    "operator": {
                      "color": "#6E4787"
                    },
                    "string": {
                      "color": "#B04E74"
                    },
                    "variable": {
                      "color": "#DB576E"
                    }
                },
                "players": [
                  {
                    "cursor": "#B04E74",
                    "background": "#190a19",
                    "selection": "#B04E7430"
                  }
                ]
            }
        }
    ]
}
