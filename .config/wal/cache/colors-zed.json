{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#121e2c90",
                "editor.background": "#121e2c90",
                "editor.foreground": "#c3c6ca",
                "text": "#c3c6ca",
                "text.muted": "#c3c6ca70",
                "text.ignored": "#c3c6ca40",
                "text.placeholder": "#c3c6ca50",
                "ignored": "#c3c6ca30",
                "element.hover": "#c3c6ca30",
                "ghost_element.hover": "#3F668330",
                "ghost_element.selected": "#3F668330",
                "ghost_element.active": "#3F668360",
                "border": "#121e2c",
                "editor.highlighted_line.background": "#3F668310",
                "editor.active_line.background": "#3F668310",
                "panel.background": "#121e2c90",
                "title_bar.background": "#121e2c90",
                "title_bar.inactive_background": "#121e2c90",
                "status_bar.background": "#121e2c90",
                "drop_target.background": "#121e2c90",
                "elevated_surface.background": "#121e2c",
                "toolbar.background": "#121e2c90",
                "tab_bar.background": "#121e2c90",
                "tab.inactive_background": "#121e2c90",
                "tab.active_background": "#3F668330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#121e2c",
                "scrollbar.thumb.background": "#3F6683",
                "editor.gutter.background": "#121e2c90",
                "terminal.background": "#121e2c10",
                "terminal.foreground": "#c3c6ca",
                "terminal.dim_foreground": "#c3c6ca",
                "terminal.bright_foreground": "#c3c6ca",
                "terminal.ansi.black": "#121e2c",
                "terminal.ansi.red": "#3F6683",
                "terminal.ansi.green": "#565D81",
                "terminal.ansi.yellow": "#665B82",
                "terminal.ansi.blue": "#4B6584",
                "terminal.ansi.magenta": "#6E6188",
                "terminal.ansi.cyan": "#A46786",
                "terminal.ansi.white": "#c3c6ca",
                "terminal.ansi.bright_black": "#616b77",
                "terminal.ansi.bright_red": "#3F6683",
                "terminal.ansi.bright_green": "#565D81",
                "terminal.ansi.bright_yellow": "#665B82",
                "terminal.ansi.bright_blue": "#4B6584",
                "terminal.ansi.bright_magenta": "#6E6188",
                "terminal.ansi.bright_cyan": "#A46786",
                "terminal.ansi.bright_white": "#c3c6ca",
                "modified": "#3F6683",
                "syntax": {
                    "attribute": {
                        "color": "#3F6683"
                    },
                    "boolean": {
                      "color": "#565D81"
                    },
                    "comment": {
                      "color": "#616b77"
                    },
                    "comment.doc": {
                      "color": "#616b77"
                    },
                    "constant": {
                      "color": "#565D81"
                    },
                    "function": {
                      "color": "#A46786"
                    },
                    "keyword": {
                      "color": "#4B6584"
                    },
                    "number": {
                      "color": "#6E6188"
                    },
                    "operator": {
                      "color": "#4B6584"
                    },
                    "string": {
                      "color": "#3F6683"
                    },
                    "variable": {
                      "color": "#565D81"
                    }
                },
                "players": [
                  {
                    "cursor": "#3F6683",
                    "background": "#121e2c",
                    "selection": "#3F668330"
                  }
                ]
            }
        }
    ]
}
